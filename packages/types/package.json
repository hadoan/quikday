{
  "name": "@quikday/types",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./auth/current-user.types": {
      "types": "./dist/auth/current-user.types.d.ts",
      "import": "./dist/auth/current-user.types.js",
      "default": "./dist/auth/current-user.types.js"
    },
    "./auth/*": {
      "types": "./dist/auth/*.d.ts",
      "import": "./dist/auth/*.js",
      "default": "./dist/auth/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.16.5",
    "@quikday/tsconfig": "workspace:*"
  }
}
