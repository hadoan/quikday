{
  "name": "@quikday/prisma",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prisma": {
    "schema": "src/schema.prisma"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "generate": "prisma generate --schema=src/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=src/schema.prisma",
    "migrate:create": "prisma migrate dev --create-only --schema=src/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=src/schema.prisma",
    "seed": "ts-node --transpile-only ./seed.ts",
    "seed-app-store": "ts-node --transpile-only ./seed-app-store.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "zod": "^3.23.8",
    "@nestjs/common": "^10.4.2"
  },
  "devDependencies": {
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "@types/node": "^20.9.1"
  }
}
