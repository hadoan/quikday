{
  "name": "@quikday/agent",
  "version": "0.1.0",
  "description": "Quik.day agent library",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./buildMainGraph": {
      "types": "./dist/buildMainGraph.d.ts",
      "import": "./dist/buildMainGraph.js",
      "require": "./dist/buildMainGraph.js",
      "default": "./dist/buildMainGraph.js"
    },
    "./llm/openai": {
      "types": "./dist/llm/openai.d.ts",
      "import": "./dist/llm/openai.js",
      "require": "./dist/llm/openai.js",
      "default": "./dist/llm/openai.js"
    },
    "./state/types": {
      "types": "./dist/state/types.d.ts",
      "import": "./dist/state/types.js",
      "require": "./dist/state/types.js",
      "default": "./dist/state/types.js"
    },
    "./observability/events": {
      "types": "./dist/observability/events.d.ts",
      "import": "./dist/observability/events.js",
      "require": "./dist/observability/events.js",
      "default": "./dist/observability/events.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir dist --rootDir .",
    "dev": "tsc -p tsconfig.json --outDir dist --rootDir . --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "quikday",
    "agent",
    "langgraph"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@nestjs/common": "^10.4.2",
    "@quikday/appstore": "workspace:*",
    "@quikday/appstore-google-calendar": "workspace:*",
    "@quikday/appstore-gmail-email": "workspace:*",
    "dotenv": "^17.2.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "typescript": "^5.9.3"
  }
}
