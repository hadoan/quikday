{
  "name": "@quikday/agent",
  "version": "0.0.0",
  "description": "Quik.day agent library",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "demo:prompts": "pnpm build && node dist/examples/modular-prompt-demo.js",
    "eval:utterances": "pnpm build && node dist/evaluation/golden-utterances.js",
    "test:goals": "tsx evaluation/test-goal-generation.ts"
  },
  "dependencies": {
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@nestjs/common": "^10.4.2",
    "@nestjs/core": "^10.4.20",
    "@quikday/appstore": "workspace:*",
    "@quikday/appstore-gmail-email": "workspace:*",
    "@quikday/appstore-google-calendar": "workspace:*",
    "@quikday/appstore-slack-messaging": "workspace:*",
    "@quikday/libs": "workspace:*",
    "@quikday/prisma": "workspace:*",
    "dotenv": "^17.2.3",
    "bullmq": "^5.7.8",
    "langfuse": "^3.20.0",
    "openai": "^4.72.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.2",
    "@quikday/tsconfig": "workspace:*",
    "@types/node": "^24.8.1",
    "typescript": "^5.9.3",
    "vitest": "^0.34.0"
  }
}
